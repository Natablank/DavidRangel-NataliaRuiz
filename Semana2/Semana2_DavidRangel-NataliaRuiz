        "###1.3 Funci√≥n de Riemann"
      ],
      "metadata": {
        "id": "30NfNJ3p1338"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def f(t):\n",
        "  return t*((t**2)-(np.pi**2))/12\n",
        "  "
      ],
      "metadata": {
        "id": "Lo1vMIAJ2Tl7"
      },
      "execution_count": 36,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def Fourier(t,T,N):\n",
        "  suma = 0\n",
        "  \n",
        "  for n in range(1,N):\n",
        "    bn, _ = integrate.quad( lambda t: f(t)*np.sin((2*np.pi*n*t)/T), -0.5*T, 0.5*T)\n",
        "    suma += (2*bn/T)**2\n",
        "  \n",
        "  return suma\n",
        "  \n"
      ],
      "metadata": {
        "id": "OpGvjDJDVcu5"
      },
      "execution_count": 48,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def sumatoria(n):\n",
        "  suma = 0\n",
        "\n",
        "  for i in range(1,n):\n",
        "    suma += 1/(i**6)\n",
        "\n",
        "  return suma"
      ],
      "metadata": {
        "id": "rUuu6weEYQI-"
      },
      "execution_count": 53,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "t = np.arange(-np.pi,np.pi,0.05)\n",
        "suma1 = Fourier(t,2*np.pi,60)\n",
        "suma2 = sumatoria(60)\n",
        "\n",
        "print(suma1,suma2)"
      ],
      "metadata": {
