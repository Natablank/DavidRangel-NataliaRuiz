        "def serie_Fourier(t,n):\n",
        "  suma = np.zeros(len(t))\n",
        "\n",
        "  for i in range(1,n):\n",
        "    suma += ((-1)**(i-1))*np.sin(i*t)/i\n",
        "\n",
        "  return 2*suma"
      ],
      "metadata": {
        "id": "9zbX0q4yaNeI"
